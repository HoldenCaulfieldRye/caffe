net: "task/clamp/train_val.prototxt"
test_iter: 128
test_interval: 50
test_compute_loss: 1
# lr for fine-tuning should be lower than when starting from scratch
base_lr: 0.0001
lr_policy: "step"
gamma: 0.1
stepsize: 10000
display: 1
max_iter: 3000
momentum: 0.9
weight_decay: 0.0005
snapshot: 5000
snapshot_prefix: "task/clamp/conv1"
solver_mode: GPU
